{
  "name": "dsj-events",
  "version": "1.0.0",
  "description": "My impressive Nuxt.js project",
  "author": "Chris Halley",
  "private": true,
  "scripts": {
    "watch-sass": "nodemon -e scss -x \"npm run build-sass\"",
    "build-sass": "node-sass --include-path scss assets/sass/main.scss assets/css/style.css",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest tests/*.test.js",
    "test-watch": "nodemon --exec 'npm test'"
  },
  "dependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@nuxtjs/axios": "^5.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "^6.26.0",
    "cloudinary-core": "^2.5.0",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.4.0",
    "nuxt": "^2.3.2",
    "nuxt-babel": "^0.1.2",
    "vuelidate": "^0.7.4"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "concurrently": "^3.5.1",
    "cross-env": "^5.0.1",
    "eslint": "^4.15.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "jest": "^23.6.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.18.6",
    "sass-loader": "^7.0.1",
    "vue-jest": "^3.0.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "transform-runtime"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.vue$": "vue-jest",
      "^.+\\.js$": "babel-jest"
    }
  }
}
